{
  "name": "@jsswpt/real-estate_ui-kit",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "watch": "storybook dev -p 3000",
    "watch:nodemon": "nodemon --quiet --watch . -e js,json,cjs,ts,tsx,md --exec",
    "watch:eslint": "yarn watch:nodemon 'yarn fix:eslint'",
    "watch:prettier": "yarn watch:nodemon 'yarn fix:prettier'",
    "fix:eslint": "yarn eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "fix:prettier": "yarn prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache'",
    "build": "tsc -b && vite build",
    "build:storybook": "storybook build",
    "test": "yarn fix:eslint && yarn fix:prettier"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "glob": "^11.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.34.8",
    "vite-plugin-dts": "^4.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "cssnano-preset-lite": "^4.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-sort": "^4.0.0",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "sass-embedded": "^1.85.0",
    "storybook": "^8.5.8",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./dist/index.css": "./dist/index.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
